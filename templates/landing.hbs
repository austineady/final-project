<header class="app-header">
  <h1 class="app-title">StoreSquare <i class="fa fa-square-o app-icon"></i></h1>
  <h4 class="app-user"><i class="fa fa-user"></i> Log In</h4>
</header>
<div class="landing-box">
  <div class="login-box  login-hidden">
    <form class="landing-form">
      <input type="text" class="user-username landing-input" placeholder="USERNAME">
      <input type="password" class="user-password landing-input" placeholder="PASSWORD">
      <paper-button type="submit" class="user-submit landing-button" raised>Log In</paper-button>
    </form>
    <paper-button raised class="new-user-button landing-button">Don't have an account? Join Us</paper-button>
  </div>
  <div class="landing-search-box search-box">
  <form class="home-search-form">
    <input type=text class="home-search-bar" placeholder="SEARCH">
  </form>
    <section class="search-results">

    </section>
  </div>
</div>
